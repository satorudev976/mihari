{
  "expo": {
    "name": "mihari",
    "slug": "mihari",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "mihari",
    "platforms": ["ios"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.jp.pripri.mihari",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": ["mihari"]
          }
        ]
      }
    },
    "plugins": ["expo-router", "expo-secure-store"]
  }
}
